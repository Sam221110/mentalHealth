<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <!-- link the main css file -->
    <link rel="stylesheet" href="css/style.css" />
    <!-- link the main js file -->
    <script type="module" src="js/app.js" defer></script>
</head>
<body>
        <!-- ai2html -->
        <section class="ai2html">
            <div class="container-fluid">
            <div class="row">
                <div class="col-12">
                <!-- This is the line that matters for import -->
                <%- include('./ai2html-output/ai2html-test.ejs'); %>
                </div>
            </div>
            </div>
        </section>

        <!-- scrolly -->
        <section id="doge-scrolly">

        <!-- define the scroll sections -->
        <% const blocks=
            [
            { 
                "filename" : "doge_1" , 
                "anno" : "_blank-slide_" 
            }, 
            { 
                "filename" : "doge_2" , 
                "anno": "<div class='h5 fw-bold'> Plastic Age </div> "
            },
                    { 
                "filename" : "doge_3" , 
                "anno": "<div class='h5 fw-bold'>Start of plastic production </div> Started from 1950 and is still going on and is used everywhere" 
                },
                { 
                    "filename" : "doge_4" , 
                    "anno": "<div class='h5 fw-bold'> Plastic emittion in oceans </div> Which continents emit the most plastic into the oceans" 
                    },
                    { 
                        "filename" : "doge_5" , 
                        "anno": "<div class='h5 fw-bold'> Asia </div> Asia produces total of 80.99 % of platic which enter the oceans" 
                        },
                        { 
                            "filename" : "doge_6" , 
                            "anno" : "_blank-slide_" 
                            },
                            { 
                                "filename" : "doge_7" , 
                                "anno": "<div class='h5 fw-bold'> 270 million tonnes per year</div>"
                                },
                                { 
                                    "filename" : "doge_8" , 
                                    "anno": "<div class='h5 fw-bold'> 275 million tonner per year</div>" 
                                    },
                                    { 
                                        "filename" : "doge_9" , 
                                        "anno": "<div class='h5 fw-bold'> 99.5 million tonner per year</div>" 
                                        },
                                        { 
                                            "filename" : "doge_10" , 
                                            "anno": "<div class='h5 fw-bold'> 31.9 million tonnes per year</div>" 
                                            },
                                            { 
                                                "filename" : "doge_11" , 
                                                "anno": "<div class='h5 fw-bold'> 8 million tonnes per year</div>" 
                                                },
                                                { 
                                                    "filename" : "doge_12" , 
                                                    "anno": "<div class='h5 fw-bold'> 10,000s to 100,000s tonnes</div>" 
                                                    },
                                                    { 
                                                        "filename" : "doge_13" , 
                                                        "anno": "<div class='h5 fw-bold'> Thank You </div> Reduce Plastic Waste" 
                                                        }
                        
                
            ] 
        %>
        <!-- every { filename, anno } object makes a new section -->

        <!-- include the component -->
        <%- include('./scrolly/scrolly.ejs', {id: "doge-scrolly" , blocks: blocks }); %>
        
        </section>
</body>
</html>